Host sauron3
    Hostname sauron
    User amano
    XAuthLocation /opt/X11/bin/xauth
    ForwardX11 yes #
    Forwardx11trusted yes
    IdentityFile ~/.ssh/id_sauron_imacnew
    ControlMaster auto #https://qiita.com/tukiyo3/items/806c4502ed7522fbcf95
    ControlPath ~/.ssh/mux-%r@%h:%p
    ControlPersist 30m



